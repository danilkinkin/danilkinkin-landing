{
    "compilerOptions": {
		"module": "esnext",
		"target": "esnext",
		"lib": [
			"esnext",
			"esnext.array"
		],
		"esModuleInterop": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./*"],
			"@/components/*": ["components/*"]
		}
    },
    "exclude": [
		"node_modules",
		".next",
		"out"
    ]
}
